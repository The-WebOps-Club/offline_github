function getAuthCode(url){ var error = url.match(/[&\?]error=([^&]+)/); if (error) { throw 'Error getting authorization code: ' + error1; } return url.match(/[&\?]code=([\w\/-]+)/)1; }  

var authCode = getAuthCode(window.location.href);
